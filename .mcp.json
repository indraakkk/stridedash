{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"],
      "env": {
        "PLAYWRIGHT_BROWSERS_PATH": "/Users/indra/.cache/ms-playwright",
        "PLAYWRIGHT_SKIP_BROWSER_DOWNLOAD": "0"
      }
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "claude-code",
        "--project",
        "/Users/indra/SaaS/stridash"
      ]
    }
  }
}
